<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.study.mappers.SBoardMapper">

	<select id="list" parameterType="com.study.sboard.SBoardVO.SBoardVO" 
	resultType="com.study.sboard.SBoardVO.SBoardVO">
		select sno, title, writer, view_cnt FROM stbl
	</select>
	
	<insert id="write" parameterType="com.study.sboard.SBoardVO.SBoardVO">
		insert into stbl
			values(#{sno}, #{writer}, #{title}, #{content}, 0)
	</insert>
</mapper>